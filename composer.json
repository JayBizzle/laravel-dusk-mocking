{
    "name": "noeldemartin/laravel-dusk-mocking",
    "description": "Mock facades in Laravel Dusk tests.",
    "keywords": ["laravel", "dusk", "testing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Noel De Martin",
            "email": "noeldemartin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/dusk": "^5.0.0",
        "laravel/framework": "~5.7.0||~5.8.0||^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0||^8.0",
        "mockery/mockery": "^1.0",
        "fzaninotto/faker": "^1.7"
    },
    "autoload": {
        "psr-4" : {
            "NoelDeMartin\\LaravelDusk\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Testing\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NoelDeMartin\\LaravelDusk\\MockingServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
