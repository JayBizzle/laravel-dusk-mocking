{
    "name": "noeldemartin/laravel-dusk-mocking",
    "description": "Mock facades in Laravel Dusk tests.",
    "keywords": ["laravel", "dusk", "testing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Noel De Martin",
            "email": "noeldemartin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/dusk": ">=3.0.0",
        "laravel/framework": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3.1",
        "fzaninotto/faker": "^1.9.1"
    },
    "autoload": {
        "psr-4" : {
            "NoelDeMartin\\LaravelDusk\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Testing\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NoelDeMartin\\LaravelDusk\\MockingServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
